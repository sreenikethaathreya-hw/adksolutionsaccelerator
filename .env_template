# Copy as .env file and fill your values below

# Choose Model Backend: 0 -> ML Dev, 1 -> Vertex
GOOGLE_GENAI_USE_VERTEXAI=1

# ML Dev backend config
GOOGLE_API_KEY=YOUR_VALUE_HERE

# Vertex backend config
GOOGLE_CLOUD_PROJECT=YOUR_VALUE_HERE
GOOGLE_CLOUD_LOCATION=YOUR_VALUE_HERE

# Database Configuration (optional)
DB_INSTANCE_CONNECTION_NAME=YOUR_VALUE_HERE  # e.g., project:region:instance
DB_USER=YOUR_VALUE_HERE
DB_PASSWORD=YOUR_VALUE_HERE
DB_NAME=YOUR_VALUE_HERE

# Staging bucket for Agent Engine deployment
STAGING_BUCKET=YOUR_VALUE_HERE

# Agent Engine ID (auto-filled after deployment)
AGENT_ENGINE_ID=YOUR_VALUE_HERE

# Secret Manager Configuration (optional)
ENV_NAME=dev